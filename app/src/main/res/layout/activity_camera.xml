<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/back"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/saaaa"
    tools:context=".camera">

    <!--    틀-->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="75dp">
    <!--상단바-->
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:layout_margin="10dp"
        android:baselineAligned="true"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="110dp"
        tools:layout_editor_absoluteY="1dp">

        <ImageButton
            android:id="@+id/imageButton6"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:background="@android:color/transparent"
            android:padding="0dp"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/noun_bell_3319638" />

        <ImageButton
            android:id="@+id/imageButton7"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginEnd="246dp"
            android:layout_marginRight="246dp"
            android:layout_marginBottom="672dp"
            android:background="@android:color/transparent"
            android:padding="0dp"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/noun_phone_1778832" />

        <ImageButton
            android:id="@+id/imageButton8"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:background="@android:color/transparent"
            android:padding="0dp"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/noun_user_2699583" />
    </LinearLayout>

        <WebView
            android:layout_width="362dp"
            android:layout_height="547dp"
            android:layout_marginLeft="25dp"/>

        <!--하단 바-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="60dp"
            android:layout_marginBottom="37dp"
            android:layout_marginRight="60dp"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/home"
                android:layout_width="52dp"
                android:layout_height="52dp"
                android:layout_marginRight="10dp"
                android:layout_weight="1"
                android:background="@android:color/transparent"
                android:layout_gravity="bottom"
                android:padding="3dp"
                android:paddingTop="10dp"
                android:scaleType="centerCrop"
                app:srcCompat="@drawable/noun_home_800461" />

            <ImageButton
                android:id="@+id/cam"
                android:layout_width="52dp"
                android:layout_height="52dp"
                android:layout_marginRight="10dp"
                android:layout_weight="1"
                android:layout_gravity="bottom"
                android:background="@android:color/transparent"
                android:padding="0dp"
                android:scaleType="centerCrop"
                app:srcCompat="@drawable/noun_video_camera_3318574" />

            <ImageButton
                android:id="@+id/message"
                android:layout_width="52dp"
                android:layout_height="52dp"
                android:layout_marginRight="10dp"
                android:layout_weight="1"
                android:background="@android:color/transparent"
                android:padding="4dp"
                android:layout_gravity="bottom"
                android:scaleType="centerCrop"
                app:srcCompat="@drawable/noun_talking_546894" />

            <ImageButton
                android:id="@+id/list"
                android:layout_width="52dp"
                android:layout_height="52dp"
                android:layout_weight="1"
                android:background="@android:color/transparent"
                android:padding="10dp"
                android:layout_gravity="bottom"
                android:scaleType="centerCrop"
                app:srcCompat="@drawable/noun_menu_2091765" />
        </LinearLayout>
</LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>